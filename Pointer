#include<iostream>
using namespace std;
int main()
{
    int a = 3;

    //---& is (Adress of operator)

    int* b= &a;

    //--->>De-referance operator

    cout<<*b<<endl;

    //pointer to pointer

    int** c=&b;
    cout<<&c<<endl;
    cout<<c<<endl;
    cout<<*c<<endl;
    cout<<**c<<endl;
}
